ion=\"rowCount > 0\">\n        <input type=\"checkbox\" title=\"Row Text\" name=\"showRowTitle\" checked condition=\"rowCount > 0\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/preschoolm5/components/_images/bg/3-bg.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/preschoolm5/components/_images/bg/1-bg.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"row title-section\" mbr-if=\"showMainTitle\">\n                <div class=\"col-12\">\n                    <h2 class=\"main-sect mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".main-sect.mbr-section-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showMainTitle\">\n                        <b>Our Staff</b>\n                    </h2>\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-12\" mbr-class=\"{'mb-4': cardAmount > 1}\">\n                    <div class=\"card\">\n                        <div class=\"card-body row justify-content-center\">\n                            <div class=\"img-container col mb-3 mb-lg-0\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/5-teachers.jpg\" alt=\"Mobirise\" class=\"img\">\n                            </div>\n                            <div class=\"col-12 col-lg-4\" mbr-if=\"showTitle || showText || showButton\">\n                                <h3 class=\"card-title main-title mbr-section-subtitle mbr-fonts-style mb-4\" mbr-theme-style=\"display-2\" data-app-selector=\".main-title.mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showTitle\">\n                                    <b>Jessica Sharp</b></h3>\n                                <p class=\"mbr-text text-main mbr-fonts-style mb-3\" mbr-theme-style=\"display-5\" data-app-selector=\".text-main\" data-app-placeholder=\"Type Text\" mbr-if=\"showText\"><b>\n                                    President</b></p>\n                                <div class=\"mbr-section-btn mb-3 mb-lg-0\" mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrBtnRemove,-mbrBtnAdd\" mbr-if=\"showButton\">\n                                    <a class=\"justify-content-center btn btn-primary\" data-app-placeholder=\"Type Text\">\n                                        LEARN MORE\n                                    </a>\n                                </div>\n                            </div>\n                            <div class=\"col-12 col-lg-4\" mbr-if=\"rowCount != 0\">\n                                <div class=\"row justify-content-center\">\n                                    <div class=\"col-12 col-lg-7\">\n                                        <h3 class=\"card-title sec-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".sec-title.mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showRowTitle\">\n                                            <b>10 years</b>\n                                        </h3>\n                                        <p class=\"mbr-text text1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".text1\" data-app-placeholder=\"Type Text\" mbr-if=\"showRowText\">\n                                            Aliquam dictum commodo eros.\n                                        </p>\n                                    </div>\n                                    <div class=\"col-12 col-lg-7 mt-3\" mbr-if=\"rowCount > 1\">\n                                        <h3 class=\"card-title sec-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".sec-title.mbr-section-subtitle\" data-app-placeholder=\"Type Text\" mbr-if=\"showRowTitle\">\n                                            <b>12 Years</b>\n                                        </h3>\n                                        <p class=\"mbr-text text2 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\" data-app-placeholder=\"Type Text\" mbr-if=\"showRowText\">\n                                            Proin pharetra massa non felis ultricies.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12\" mbr-if=\"cardAmount > 1\">\n                    <div class=\"card\">\n                        <div class=\"card-body row justify-content-center\">\n                            <div class=\"img-container col mb-3 mb-lg-0\" mbr-if=\"showImage\">\n                           